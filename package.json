  "devDependencies": {
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/jest": "^28.0.0",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.0.0",
    "@types/nodemailer": "^6.4.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^2.0.0",
    "jest": "^28.0.0",
    "prettier": "^2.0.0",
    "prisma": "^6.5.0",
    "supertest": "^6.0.0",
    "ts-jest": "^28.0.0",
    "ts-loader": "^9.0.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.0.0",
    "typescript": "^5.8.2"
  }
